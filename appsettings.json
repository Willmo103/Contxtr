{
    "Contxtr": {
        "ConfigurationProvider": "json",
        "LanguageMap": {
            "Extensions": {
                ".cs": "csharp",
                ".js": "javascript",
                ".ts": "typescript",
                ".html": "html",
                ".css": "css",
                ".scss": "scss",
                ".json": "json",
                ".xml": "xml",
                ".yml": "yaml",
                ".yaml": "yaml",
                ".md": "markdown",
                ".txt": "plaintext",
                ".csproj": "xml",
                ".csv": "csv",
                ".sql": "sql",
                ".sh": "shellscript",
                ".bat": "batch",
                ".ps1": "powershell",
                ".psm1": "powershell",
                ".psd1": "powershell",
                ".ps1xml": "powershell",
                ".xaml": "xaml",
                ".config": "xml",
                ".editorconfig": "editorconfig",
                ".dockerignore": "dockerignore",
                "dockerfile": "dockerfile"
            }
        },
        "Ignore": {
            "EnableGitIgnore": true,
            "EnableSystemIgnores": true,
            "Patterns": [
                {
                    ".git": "Anything in the .git folder",
                    "node_modules": "node_modules",
                    "bin": "bin",
                    "obj": "obj",
                    ".env": "environment files",
                    "package-lock.json": "package-lock.json",
                    ".vs": "vs",
                    ".vscode": "vscode",
                    ".idea": "idea",
                    "__pycache__": "Python cache files",
                    ".pytest_cache": "pytest cache files",
                    ".mypy_cache": "mypy cache files",
                    ".tox": "tox",
                    ".venv": "venv",
                    "venv": "venv",
                    ".vscode-test": "vscode-test",
                    "packages": "packages in VS",
                    ".gitignore": ".gitignore files",
                    "*codebase*.md": "md files that I  create",
                    "output": "output folder",
                    "dist": "dist folder"
                }
            ]
        },
        "Storage": {
            "BaseDirectory": "./documents",
            "EnableVersioning": true,
            "MaxVersions": 5
        },
        "Processing": {
            "ParallelProcessing": true,
            "MaxDegreeOfParallelism": -1,
            "FollowSymlinks": false
        }
    },
    "ConnectionStrings": {
        "ContxtrConfig": "Server=...;Database=..."
    }
}